{
	"manifest_version": 3,
	"name": "My Carbon Trigger",
	"version": "0.1.0",
	"host_permissions": [
		"https://random-d.uk/*",
		"<all_urls>"
	],
	"background": {
		"service_worker": "background.js"
	},
	"permissions": ["action"],
	"action": {
		"default_popup": "index.html",
		"default_icon": {
		  	"16": "icons/duck.png",
		  	"48": "icons/duck.png",
		  	"128": "icons/duck.png"
		}
	},
	"icons": {
		"16": "icons/duck.png",
		"48": "icons/duck.png",
		"128": "icons/duck.png"
	}
	
}
